modules = ["python-3.11"]

[nix]
channel = "stable-24_05"
packages = ["cargo", "freetype", "glibcLocales", "lcms2", "libiconv", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "openssl", "pkg-config", "rustc", "tcl", "tk", "zlib"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "gcPanel Sidebar"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "gcPanel Enterprise"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "gcPanel Fixed"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "gcPanel Clean"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "gcPanel Working"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "gcPanel Enterprise Ultimate"

[[workflows.workflow]]
name = "gcPanel Sidebar"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app_simple.py --server.port 5000"
waitForPort = 5000

[[workflows.workflow]]
name = "gcPanel Enterprise"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app_enterprise.py --server.port 5000"
waitForPort = 5000

[[workflows.workflow]]
name = "gcPanel Fixed"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app_fixed.py --server.port 5000"
waitForPort = 5000

[[workflows.workflow]]
name = "gcPanel Clean"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app_clean_modular.py --server.port 5000"
waitForPort = 5000

[[workflows.workflow]]
name = "gcPanel Working"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app_working.py --server.port 5000"
waitForPort = 5000

[[workflows.workflow]]
name = "gcPanel Enterprise Ultimate"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run gcpanel_enterprise.py --server.port 5000"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80
